#pytha
pytha <- function(s, t) {
a <- s ^ 2 - t ^ 2
b <- 2 * s * t
c <- s ^ 2 + t ^ 2
cat('Pythagorean theorem : 3 variables : ', a, b, c)
}
pytha(2, 1)
